#include<iostream>
using namespace std;
int main(){

   int n;
   cin>>n;
   int t1=1;
   int t2=1;
   int nt=0;
   for(int i=0;i<n;i++){
    for(int j=1;j<=i+1;j++){
        if(i==0){
            cout<<"0";
        }
        else if(i==1){
            cout<<t1<<" ";
        }
        else{
        nt=t1+t2;
        t1=t2;
        t2=nt;

        cout << nt << " ";
        }
    }
    cout<<endl;
   }
}

